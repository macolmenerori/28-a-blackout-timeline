{
  "name": "28-a-blackout-timeline",
  "version": "1.0.0",
  "type": "module",
  "description": "Full timeline of the blackout that affected Spain and Portugal on April 28, 2025.",
  "engines": {
    "node": ">=24.0.0"
  },
  "author": "macolmenerori",
  "license": "MIT",
  "packageManager": "pnpm@10.22.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/macolmenerori/28-a-blackout-timeline"
  },
  "scripts": {
    "dev": "vite-react-ssg dev",
    "build": "tsx scripts/generate-sitemap.ts && tsc -b && vite-react-ssg build",
    "prettify": "prettier -w --log-level=silent src/",
    "preview": "vite preview",
    "lint": "eslint --fix ./src/",
    "types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "verify": "pnpm lint && pnpm prettify && pnpm types && pnpm audit --prod && pnpm test && pnpm build"
  },
  "keywords": [
